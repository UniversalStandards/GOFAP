# Session Configuration
SESSION_SECRET=your_session_secret_key

# Feature Flags
ENABLE_PAYMENT_PROCESSING=false
ENABLE_EXTERNAL_INTEGRATIONS=false
ENABLE_ADVANCED_ANALYTICS=false