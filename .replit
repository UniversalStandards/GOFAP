
run = "python3 main.py"

[nix]
channel = "stable-22_11"

[languages.python3]
pattern = "**/*.py"

[languages.python3.languageServer]
start = "pylsp"

[env]
PYTHONPATH = "${REPL_HOME}/src:${PYTHONPATH}"
FLASK_APP = "main.py"
FLASK_ENV = "development"

[gitHubImport]
requiredFiles = [".replit", "replit.nix", "main.py"]

[deployment]
run = ["python3", "main.py"]
deploymentTarget = "cloudrun"
